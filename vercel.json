{
  "version": 2,
  "builds": [
    {
      "src": "api/main.py",
      "use": "@vercel/python"
    }
  ],
  "functions": {
    "api/main.py": {
      "runtime": "@vercel/python@3.1.0",
      "memory": 1024,
      "maxDuration": 300
    }
  },
  "routes": [
    {
      "src": "/(.*)",
      "dest": "api/main.py"
    }
  ],
  "env": {
    "APP_MODULE": "api.main:app"
  }
}
